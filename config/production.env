# Production environment configuration
ENVIRONMENT=production
AWS_REGION=us-east-1

# Database configuration
DB_HOST=${DB_HOST}
DB_PORT=5432
DB_NAME=healthcare_analysis_prod
DB_USER=${DB_USER}
DB_PASSWORD=${DB_PASSWORD}

# S3 configuration
S3_BUCKET_IMAGES=healthcare-mri-images-prod
S3_BUCKET_RESULTS=healthcare-processed-results-prod

# SQS configuration
SQS_QUEUE_NAME=mri-processing-queue-prod

# Step Functions configuration
STEP_FUNCTION_ARN=${STEP_FUNCTION_ARN}

# SageMaker endpoints
SAGEMAKER_SEGMENTATION_ENDPOINT=mri-segmentation-endpoint-prod
SAGEMAKER_VLM_ENDPOINT=huggingface-vlm-endpoint-prod

# Bedrock configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# OpenSearch configuration
OPENSEARCH_ENDPOINT=${OPENSEARCH_ENDPOINT}
OPENSEARCH_INDEX=medical-knowledge-prod

# Logging configuration
LOG_LEVEL=INFO
CLOUDWATCH_LOG_GROUP=/aws/lambda/healthcare-analysis-prod