# Development environment configuration
ENVIRONMENT=development
AWS_REGION=us-east-1

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=healthcare_analysis_dev
DB_USER=dev_user
DB_PASSWORD=dev_password

# S3 configuration
S3_BUCKET_IMAGES=healthcare-mri-images-dev
S3_BUCKET_RESULTS=healthcare-processed-results-dev

# SQS configuration
SQS_QUEUE_NAME=mri-processing-queue-dev

# Step Functions configuration
STEP_FUNCTION_ARN=arn:aws:states:us-east-1:123456789012:stateMachine:MRIAnalysisPipeline-dev

# SageMaker endpoints
SAGEMAKER_SEGMENTATION_ENDPOINT=mri-segmentation-endpoint-dev
SAGEMAKER_VLM_ENDPOINT=huggingface-vlm-endpoint-dev

# Bedrock configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# OpenSearch configuration
OPENSEARCH_ENDPOINT=https://search-medical-knowledge-dev.us-east-1.es.amazonaws.com
OPENSEARCH_INDEX=medical-knowledge-dev

# Logging configuration
LOG_LEVEL=DEBUG
CLOUDWATCH_LOG_GROUP=/aws/lambda/healthcare-analysis-dev