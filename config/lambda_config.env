# Lambda function environment variables for S3 event handler

# AWS Configuration
AWS_REGION=us-east-1

# SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/mri-processing-queue

# Database Configuration
DB_HOST=healthcare-analysis-db.cluster-xyz.us-east-1.rds.amazonaws.com
DB_PORT=5432
DB_NAME=healthcare_analysis
DB_USERNAME=postgres
# DB_PASSWORD should be set via AWS Secrets Manager or Parameter Store

# Step Functions Configuration
STEP_FUNCTIONS_ARN=arn:aws:states:us-east-1:123456789012:stateMachine:MRIAnalysisPipeline

# Logging Configuration
LOG_LEVEL=INFO
DB_ECHO=false

# Connection Pool Configuration
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600

# File Validation Configuration
MAX_FILE_SIZE_MB=500
SUPPORTED_EXTENSIONS=.nii,.nii.gz,.dcm

# SageMaker Configuration
SAGEMAKER_ENDPOINT_NAME=mri-segmentation-endpoint
MODEL_TIMEOUT_SECONDS=300
OUTPUT_BUCKET=healthcare-processed-results