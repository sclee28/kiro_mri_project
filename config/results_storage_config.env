# Results Storage and Notification Lambda Configuration

# AWS Configuration
AWS_REGION=us-east-1

# Database Configuration
DB_HOST=healthcare-analysis-db.cluster-xyz.us-east-1.rds.amazonaws.com
DB_PORT=5432
DB_NAME=healthcare_analysis
DB_USERNAME=postgres
# DB_PASSWORD should be set via AWS Secrets Manager or Parameter Store

# Notification Configuration
NOTIFICATION_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:healthcare-job-completion
ENABLE_NOTIFICATIONS=true

# S3 Configuration
OUTPUT_BUCKET=healthcare-processed-results

# Logging Configuration
LOG_LEVEL=INFO
DB_ECHO=false

# Connection Pool Configuration
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600