# SageMaker Models Configuration

# MRI Segmentation Model Configuration
SEGMENTATION_MODEL_NAME=mri-segmentation-model
SEGMENTATION_ENDPOINT_NAME=mri-segmentation-endpoint
SEGMENTATION_MODEL_DATA_URL=s3://model-artifacts/mri-segmentation/model.tar.gz
SEGMENTATION_INSTANCE_TYPE=ml.g4dn.xlarge
SEGMENTATION_INSTANCE_COUNT=1
SEGMENTATION_MAX_INSTANCE_COUNT=4
SEGMENTATION_AUTOSCALING_TARGET=70.0

# VLM Model Configuration
VLM_MODEL_NAME=huggingface-vlm-model
VLM_ENDPOINT_NAME=huggingface-vlm-endpoint
VLM_MODEL_ID=microsoft/BiomedCLIP-PubMedBERT_256-vit_base_patch16_224
VLM_TASK=image-to-text
VLM_INSTANCE_TYPE=ml.g5.xlarge
VLM_INSTANCE_COUNT=1
VLM_MAX_INSTANCE_COUNT=3
VLM_AUTOSCALING_TARGET=60.0

# Monitoring Configuration
MONITORING_ALERT_SNS_TOPIC=sagemaker-model-alerts
MONITORING_CPU_THRESHOLD=85
MONITORING_MEMORY_THRESHOLD=85
MONITORING_GPU_THRESHOLD=90
MONITORING_LATENCY_THRESHOLD=5000
MONITORING_ERROR_THRESHOLD=10